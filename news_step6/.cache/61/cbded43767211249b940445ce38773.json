\n}; //end of newsDetail\r\n\r\nrouter = () => {\r\n  const routerPath = location.hash;\r\n  console.log(routerPath);\r\n  if (routerPath === \"\") {\r\n    // === : 타입까지 비교\r\n    getNewsData(); //글목록 보기\r\n  } else {\r\n    newsDetail(); //글 내용 보기\r\n  }\r\n};\r\n\r\naddEventListener(\"hashchange\", router);\r\nrouter();\r\n"},"lineCount":null}},"error":null,"hash":"609704d6d3e6543e3e4d8e1c281db828","cacheData":{"env":{}}}